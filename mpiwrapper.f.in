      subroutine mpiwrapper_export_fortran_constants
      implicit none
      include "mpif.h"

#define MT(TYPE) WPI_##TYPE
#define CONSTANT(TYPE, NAME) \
      &     integer WPI_##NAME \
      &     common /WPI_##NAME/ WPI_##NAME
#include "mpi-constants-f.inc"
#undef CONSTANT
#undef MT

#define MT(TYPE) WPI_##TYPE
#define CONSTANT(TYPE, NAME) \
      &     WPI_##NAME = MPI_##NAME
#include "mpi-constants-f.inc"
#undef CONSTANT
#undef MT

      end
